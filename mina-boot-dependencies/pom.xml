                <artifactId>mina-boot-common-exception</artifactId>
                <version>${mina.version}</version>
            </dependency>
            <dependency>
                <groupId>cn.mina.boot</groupId>
                <artifactId>mina-boot-web-auth-starter</artifactId>
                <version>${mina.version}</version>
            </dependency>
            <dependency>
                <groupId>cn.mina.boot</groupId>
                <artifactId>mina-boot-web-starter</artifactId>
                <version>${mina.version}</version>
