                <artifactId>mina-boot-starter</artifactId>
                <version>${mina.version}</version>
            </dependency>
            <dependency>
                <groupId>cn.mina.boot</groupId>
                <artifactId>mina-boot-common-util</artifactId>
                <version>${mina.version}</version>
            </dependency>
            <dependency>
                <groupId>cn.mina.boot</groupId>
                <artifactId>mina-boot-common-core</artifactId>
                <version>${mina.version}</version>
