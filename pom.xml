<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">    <modelVersion>4.0.0</modelVersion>    <groupId>cn.mina.boot</groupId>    <artifactId>mina-parent</artifactId>    <version>${mina.version}</version>    <packaging>pom</packaging>    <name>mina-boot-parent</name>    <description>mina-parent</description>    <modules>        <module>mina-boot-example</module>        <module>mina-boot-common</module>        <module>mina-boot-oauth2</module>        <module>mina-boot-data</module>        <module>mina-boot-web</module>        <module>mina-boot-cache</module>        <module>mina-boot-config</module>        <module>mina-boot-message</module>        <module>mina-boot-oss</module>        <module>mina-boot</module>        <module>mina-boot-dependencies</module>        <module>mina-boot-job</module>        <module>mina-boot-admin</module>        <module>mina-boot-logging</module>        <module>mina-boot-protocol</module>        <module>mina-boot-ratelimit</module>        <module>mina-boot-docs</module>    </modules>    <properties>        <java.version>1.8</java.version>        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>        <maven.compiler.source>11</maven.compiler.source>        <maven.compiler.target>11</maven.compiler.target>        <mina.version>0.0.1-SNAPSHOT</mina.version>    </properties>    <dependencyManagement>        <dependencies>            <dependency>                <groupId>cn.mina.boot</groupId>                <artifactId>mina-boot-dependencies</artifactId>                <version>${mina.version}</version>                <type>pom</type>                <scope>import</scope>            </dependency>        </dependencies>    </dependencyManagement>    <build>        <plugins>            <plugin>                <groupId>org.springframework.boot</groupId>                <artifactId>spring-boot-maven-plugin</artifactId>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-compiler-plugin</artifactId>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-deploy-plugin</artifactId>            </plugin>        </plugins>        <pluginManagement>            <plugins>                <plugin>                    <groupId>org.apache.maven.plugins</groupId>                    <artifactId>maven-compiler-plugin</artifactId>                    <version>3.1</version>                    <configuration>                        <source>8</source>                        <target>8</target>                    </configuration>                </plugin>                <plugin>                    <groupId>org.apache.maven.plugins</groupId>                    <artifactId>maven-deploy-plugin</artifactId>                    <version>2.8.2</version>                </plugin>                <plugin>                    <groupId>org.springframework.boot</groupId>                    <artifactId>spring-boot-maven-plugin</artifactId>                    <version>2.7.2</version>                    <configuration>                        <excludes>                            <exclude>                                <groupId>org.projectlombok</groupId>                                <artifactId>lombok</artifactId>                            </exclude>                        </excludes>                    </configuration>                </plugin>                <plugin>                    <groupId>com.spotify</groupId>                    <artifactId>dockerfile-maven-plugin</artifactId>                    <version>1.4.13</version>                    <!--                    因该配置会引起nocalhost插件，在打包时触发镜像构建指令，故暂时注释，构建镜像可以单独时候用dockerfile插件build-->                    <!--                    <executions>-->                    <!--                        <execution>-->                    <!--                            <id>default</id>-->                    <!--                            <goals>-->                    <!--                                <goal>build</goal>-->                    <!--                                <goal>push</goal>-->                    <!--                            </goals>-->                    <!--                        </execution>-->                    <!--                    </executions>-->                    <configuration>                        <repository>registry.cn-hangzhou.aliyuncs.com/haot_repo/${project.artifactId}</repository>                        <tag>${project.version}</tag>                        <useMavenSettingsForAuth>true</useMavenSettingsForAuth>                        <buildArgs>                            <JAR_FILE>${project.build.finalName}.jar</JAR_FILE>                        </buildArgs>                    </configuration>                </plugin>                <!--                <plugin>-->                <!--                    <groupId>cn.mina.boot</groupId>-->                <!--                    <artifactId>mina-dependency-maven-plugin</artifactId>-->                <!--                    <version>1.0.0-SNAPSHOT</version>-->                <!--                    <configuration>-->                <!--                        <outputFile>src/main/resources/dependency.txt</outputFile>-->                <!--                    </configuration>-->                <!--                    <executions>-->                <!--                        <execution>-->                <!--                            <id>generate-dependency.txt</id>-->                <!--                            <phase>generate-resources</phase>-->                <!--                            <goals>-->                <!--                                <goal>tree</goal>-->                <!--                            </goals>-->                <!--                        </execution>-->                <!--                        <execution>-->                <!--                            <id>remove-dependency.txt</id>-->                <!--                            <phase>compile</phase>-->                <!--                            <goals>-->                <!--                                <goal>remove-tree</goal>-->                <!--                            </goals>-->                <!--                        </execution>-->                <!--                    </executions>-->                <!--                </plugin>-->            </plugins>        </pluginManagement>    </build>    <distributionManagement>        <repository>            <id>rdc-releases</id>            <url>https://packages.aliyun.com/maven/repository/2244391-release-zNHCbw/</url>            <releases>                <enabled>true</enabled>            </releases>        </repository>        <snapshotRepository>            <id>rdc-snapshots</id>            <url>https://packages.aliyun.com/maven/repository/2244391-snapshot-OqPMVa/</url>        </snapshotRepository>    </distributionManagement>    <repositories>        <repository>            <id>central</id>            <url>https://maven.aliyun.com/nexus/content/groups/public</url>            <releases>                <enabled>true</enabled>            </releases>            <snapshots>                <enabled>true</enabled>            </snapshots>        </repository>        <repository>            <id>rdc-releases</id>            <url>https://packages.aliyun.com/maven/repository/2244391-release-zNHCbw/</url>            <releases>                <enabled>true</enabled>            </releases>            <snapshots>                <enabled>false</enabled>            </snapshots>        </repository>        <repository>            <id>rdc-snapshots</id>            <url>https://packages.aliyun.com/maven/repository/2244391-snapshot-OqPMVa/</url>            <releases>                <enabled>false</enabled>            </releases>            <snapshots>                <enabled>true</enabled>            </snapshots>        </repository>    </repositories>    <pluginRepositories>        <pluginRepository>            <id>central</id>            <url>https://maven.aliyun.com/nexus/content/groups/public</url>            <releases>                <enabled>true</enabled>            </releases>            <snapshots>                <enabled>true</enabled>            </snapshots>        </pluginRepository>    </pluginRepositories></project>